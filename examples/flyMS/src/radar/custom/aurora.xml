<?xml version='1.0'?>
<mavlink>
     <messages>
	<message id="180" name="RADAR">
		<description>This message encodes all of the raw sensor data from the radar driver.</description>
		<field type="int32_t" name="n">Radar sensor number of elements</field>
		<field type="uint16_t[5]" name="distance">Radar sensor distance</field>
		<field type="uint8_t[5]" name="up_mag">Radar sensor up magnitude</field>
		<field type="uint8_t[5]" name="down_mag">Radar sensor down magnitude</field>
	</message>
	<message id="181" name="RADAR_FILTERED">
		<description>This message encodes all of the filtered sensor data from the radar driver.</description>
		<field type="float[5]" name="distance">Filtered distance</field>
		<field type="float[5]" name="confidence">Filtered confidence</field>
		<field type="float[5]" name="velocity">Filtered velocity</field>
	</message>
     </messages>
</mavlink>
